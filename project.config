[project]
	description = LibreOffice dictionaries repository
[receive]
	requireChangeId = true
[submit]
	action = cherry pick
	mergeContent = true
[access]
	inheritFrom = Dev-ACL-Template
[access "refs/*"]
	read = group ReplicateFDO
	read = group ReplicateGithub
	read = group ReplicateGitorious
	read = group ReplicateLaunchpad
[allowSuperproject "core"]
	matching = refs/heads/distro/capgemini/cg-4.1:refs/heads/distro/capgemini/cg-4.1
