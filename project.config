[project]
	description = LibreOffice dictionaries repository
[receive]
	requireChangeId = true
[submit]
	action = cherry pick
	mergeContent = true
[access]
	inheritFrom = Dev-ACL-Template
[access "refs/*"]
	read = group Gitorious Mirror
	read = group Launchpad Mirror
	read = group ReplicateFDO
	read = group ReplicateGithub
[allowSuperproject "core"]
	matching = refs/heads/distro/mimo/mimo-5-4-7-2:refs/heads/distro/mimo/mimo-5-4-7-2
